# Debug Configuration
# Enable debug logging (only works in development mode)
NEXT_PUBLIC_DEBUG_ENABLED=true

# Debug categories (comma-separated or "all")
# Available: auth, api, viewmodel, usecase, service, ui, all
NEXT_PUBLIC_DEBUG_CATEGORIES="all"

# Examples for specific debugging:
# - Authentication only: NEXT_PUBLIC_DEBUG_CATEGORIES="auth,usecase"
# - API calls only: NEXT_PUBLIC_DEBUG_CATEGORIES="api"
# - Full flow: NEXT_PUBLIC_DEBUG_CATEGORIES="all"

# AWS Cognito Configuration
# Add your Cognito settings here
# NEXT_PUBLIC_COGNITO_REGION=eu-central-1
# NEXT_PUBLIC_COGNITO_USER_POOL_ID=your-user-pool-id
# NEXT_PUBLIC_COGNITO_CLIENT_ID=your-client-id

# API Configuration
# NEXT_PUBLIC_API_BASE_URL=https://your-api.execute-api.region.amazonaws.com/prod
